    "connect-mongo": "^2.0.1",
    "dotenv": "^6.0.0",
    "draft-js": "^0.10.5",
    "draft-js-export-html": "^1.2.0",
    "draft-js-plugins-editor": "^2.1.1",
    "draft-js-single-line-plugin": "^2.0.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fs": "^0.0.1-security",
    "hex-rgb": "^3.0.0",
    "js-cookie": "^2.2.0",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.10",
    "material-design-icons": "^3.0.1",
    "mongoose": "^5.2.2",
    "nodemailer": "^4.6.7",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "polished": "^1.9.3",
    "query-string": "^6.1.0",
    "randomstring": "^1.1.5",
    "rc-tooltip": "^3.7.2",
    "react": "^16.4.1",
    "react-card-flip": "^0.2.1",
    "react-detect-offline": "^2.1.2",
    "react-dom": "^16.4.1",
    "react-dropzone": "^4.3.0",
    "react-ga": "^2.5.3",
    "react-portal-tooltip": "^2.4.0",
