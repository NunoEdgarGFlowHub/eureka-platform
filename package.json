    "react-scripts": "1.1.4",
    "react-select": "^2.0.0",
    "react-sizeme": "^2.5.2",
    "react-text-truncate": "^0.13.1",
    "react-toastify": "^4.2.0",
    "slugify": "^1.3.1",
    "solc": "^0.4.24",
    "styled-components": "^3.3.3",
    "then-mongo": "^3.0.2",
