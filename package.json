{
  "name": "eureka-platform",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@kennethormandy/react-flipcard": "^0.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fs": "^0.0.1-security",
    "js-cookie": "^2.2.0",
    "mongoose": "^5.2.2",
    "nodemailer": "^4.6.7",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "polished": "^1.9.3",
    "react": "^16.4.1",
    "react-card-flip": "^0.2.1",
    "react-detect-offline": "^2.1.2",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-select": "^2.0.0",
    "solc": "^0.4.24",
    "styled-components": "^3.3.3",
    "then-mongo": "^3.0.2",
    "truffle-contract": "3.0.5",
    "web3": "^1.0.0-beta.34"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run lint && ava --serial",
    "test-sc": "ava --serial test/integration/smartcontracts",
    "test-ci": "if [ \"${CIRCLE_BRANCH}\" = \"dev\" ]; then ava --serial ./test/unit/NormalTests.js; elif [ \"${CIRCLE_BRANCH}\" = \"master\" ]; then ava --serial ./test/unit/NormalTests.js; else ava --serial;fi",
    "lint": "eslint **/*.mjs --ignore-path .gitignore",
    "prettier": "prettier --write --single-quote --no-bracket-spacing --no-jsx-bracket-same-line --no-use-tabs **/*.mjs",
    "eject": "react-scripts eject",
    "dev-client": "react-scripts start",
    "dev-server": "set NODE_ENV=development && nodemon --exec \"node --experimental-modules\" src/backend babel-node",
    "dev-contracts": "set NODE_ENV=development && nodemon --exec \"node --experimental-modules\" src/backend/web3/deployer-script.mjs babel-node",
    "dev-run": "set NODE_ENV=development && nodemon --exec \"node --experimental-modules\" localDevTests/index.mjs babel-node"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^5.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.6"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "useTabs": false
  }
}
