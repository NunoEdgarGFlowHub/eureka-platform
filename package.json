    "web3": "^1.0.0-beta.34"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run lint && ava --serial",
    "test-sc": "ava --serial test/integration/smartcontracts",
    "test-ci": "if [ \"${CIRCLE_BRANCH}\" = \"dev\" ]; then ava --serial ./test/unit/NormalTests.js; elif [ \"${CIRCLE_BRANCH}\" = \"master\" ]; then ava --serial ./test/unit/NormalTests.js; else ava --serial;fi",
    "test-db": "ava --serial test/db",
    "test-int": "cross-env BC_NETWORK=ganache NODE_ENV=development npm run deploy-contracts && ava --serial test/integration/sc-event-listening-test.js",
    "lint": "eslint **/*.mjs --ignore-path .gitignore",
    "prettier": "prettier --write --single-quote --no-bracket-spacing --no-jsx-bracket-same-line --no-use-tabs **/*.mjs",
    "eject": "react-scripts eject",
    "dev-client": "react-scripts start",
    "kov-server": "cross-env BC_NETWORK=kovan NODE_ENV=development nodemon --exec \"node --experimental-modules\" src/backend babel-node",
    "gan-server": "cross-env BC_NETWORK=ganache NODE_ENV=development nodemon --exec \"node --experimental-modules\" src/backend babel-node",
    "deploy-gan-server": "cross-env BC_NETWORK=ganache NODE_ENV=development npm run deploy-contracts && npm run gan-server",
    "deploy-contracts": "cross-env BC_NETWORK=ganache NODE_ENV=development node --experimental-modules src/smartcontracts/deployment/deployer-script.mjs babel-node",
    "nodemon-dev": "cross-env BC_NETWORK=ganache NODE_ENV=development nodemon --exec \"node --experimental-modules\" localDevTests/index.mjs babel-node"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.6"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "useTabs": false
  }
}
